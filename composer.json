{
    "require": {
        "php-di/php-di": "^6.3",
        "ramsey/uuid": "^4.1",
        "selective/basepath": "^2.0",
        "slim/php-view": "^3.0",
        "slim/psr7": "^1.3",
        "slim/slim": "4.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
